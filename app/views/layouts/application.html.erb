<!DOCTYPE html>
<html>
<head>
  <title>PlaylistProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <ul class="nav_bar">
    <li class="soundbaselogo">
      <%= link_to 'SoundBase', welcome_index_path %>
    </li>
    <li class="home_link">
      <%= link_to 'Feed', playlists_path %>
    </li>
    <li class="new_link">
      <%= link_to 'New Playlist', new_playlist_path %>

    </li>
     <li class="logout">
      <%= link_to 'Log Out', destroy_user_session_path, :method => :delete %>

    </li>
  </ul>
<%= current_user.inspect %>
<%= yield %>

</body>
</html>
